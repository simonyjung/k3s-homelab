{
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    "group:monorepos",
    "group:recommended",
    "mergeConfidence:age-confidence-badges",
    "replacements:all",
    "workarounds:all"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^apps/.*/.*\\.yaml$"],
      "matchStrings": ["image: (?<depName>.*?):(?<currentValue>.*)"],
      "datasourceTemplate": "docker"
    }
  ]
}